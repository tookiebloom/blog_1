<!-- TEMPLATE FOR MEDIA FILE CONTAINER -->
<script type="text/template" id="media_files_template">
<div class="attach-media-overlay">

	<h2>Pick which media files to associate with this post!</h2>

	<div class="row media_container">~{media}~</div>

	<br><br>

	<div class="media-buttons">
		<a href="#" class="admin-button save" >Save! </a>
		<a href="#" class="admin-button hide_overlay" >Cancel</a>
	</div>
</div>

</script>


<!-- TEMPLATE FOR MEDIA FILE COLUMN -->

<script type="text/template" id="media_file_column">
	<div class="cell col-1-4 media-file-container">
		~{media_files}~
	</div>
</script>


<!-- TEMPLATE FOR SINGLE MEDIA FILE -->
<script type="text/template" id="single_media_file">
	<div class="img-wrapper ~{additional_classes}~"  data-image-id="~{id}~">
		<img src="~{url}~"  />
		<i class="fa fa-check-square-o"></i>
	</div>
</script>


<!-- TEMPLATE FOR DISPLAYING IMAGE INFO -->
<script type="text/template" id="single_media_info">
	<div class="img-info-wrapper">
		<img src="~{src}~"  />

		<h4>Image SRC:</h4>
		<div class="img-info-src">~{src}~</div>

		<a class="admin-button ~{button_class}~" data-media-id="~{id}~", data-media-src="~{src}~">~{button_label}~</a>
	</div>
</script>



<!-- TEMPLATE FOR DISPLAYING IMAGE INFO -->
<script type="text/template" id="tag_color_picker_options">
	<ul>
		<li>No&nbsp;Color</li>
		<li class="color-red">Red</li>
		<li class="color-blue">Blue</li>
		<li class="color-green">Green</li>
	</ul>
</script>


</html>
